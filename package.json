{
  "name": "markdown-renderer",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev:react": "bunx rspack serve --config rspack.react.config.js",
    "dev:vue": "bunx rspack serve --config rspack.vue.config.js",
    "build:react": "bunx rspack build --config rspack.react.config.js",
    "build:vue": "bunx rspack build --config rspack.vue.config.js",
    "build:types": "tsc --emitDeclarationOnly",
    "type-check": "tsc --noEmit",
    "format": "biome format --write .",
    "lint": "biome check .",
    "lint:fix": "biome check --apply ."
  },
  "dependencies": {
    "@vue/runtime-core": "^3.5.0",
    "@vue/runtime-dom": "^3.5.0",
    "hast-util-to-jsx-runtime": "^2.2.0",
    "highlight.js": "11.11.1",
    "katex": "0.16.19",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "rehype-highlight": "^7.0.0",
    "rehype-katex": "^7.0.0",
    "rehype-stringify": "^10.0.0",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.0.0",
    "unified": "^11.0.4",
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "7.26.0",
    "@babel/preset-react": "7.26.3",
    "@babel/preset-typescript": "7.26.0",
    "@biomejs/biome": "1.5.1",
    "@rspack/cli": "^1.1.8",
    "@rspack/core": "^1.1.8",
    "@types/node": "^20.10.8",
    "@types/react": "19.0.4",
    "@types/react-dom": "19.0.2",
    "@vue/babel-plugin-jsx": "1.2.5",
    "@vue/compiler-sfc": "^3.5.0",
    "babel-loader": "^9.1.3",
    "css-loader": "7.1.2",
    "html-webpack-plugin": "5.6.3",
    "style-loader": "4.0.0",
    "typescript": "^5.3.3",
    "vue-loader": "^17.4.2",
    "webpack": "5.97.1",
    "webpack-dev-server": "5.2.0"
  },
  "peerDependencies": {
    "vue": "^3.5.0",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  }
}